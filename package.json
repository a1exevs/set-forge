{
  "name": "set-forge",
  "version": "0.1.2",
  "description": "A workout planning and strength tracking app that lets you build exercise lists, log sets and weights, rate perceived effort, and visualize progress over time.",
  "private": true,
  "author": "AlexEvs",
  "bugs": {
    "url": "https://github.com/a1exevs/set-forge/issues"
  },
  "keywords": [
    "exercise lists",
    "workout",
    "training",
    "progress visualization",
    "sport",
    "exercises",
    "workout planning"
  ],
  "engines": {
    "node": "22.20.0",
    "npm": "10.9.3"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "check-deps": "npm outdated",
    "upgrade-deps": "npm upgrade --latest",
    "format": "prettier --write \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx,css,scss}\"",
    "format:check": "prettier --check \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx,css,scss}\"",
    "lint": "eslint --config eslint.config.ts \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"{src,.storybook,linter,tests,scripts}/**/*.{ts,tsx}\" --fix",
    "test:unit": "cross-env TS_NODE_PROJECT=tsconfig.jest.json jest --config tests/unit/config/config.ts",
    "test:unit-cov": "cross-env TS_NODE_PROJECT=tsconfig.jest.json jest --config tests/unit/config/config.ts --coverage",
    "test:snap": "cross-env TS_NODE_PROJECT=tsconfig.jest.json jest --config tests/snap/config/config.ts",
    "test:snap-cov": "cross-env TS_NODE_PROJECT=tsconfig.jest.json jest --config tests/snap/config/config.ts --coverage",
    "test:snap-update": "cross-env TS_NODE_PROJECT=tsconfig.jest.json jest --updateSnapshot --config tests/snap/config/config.ts",
    "test:e2e": "playwright test --config=tests/e2e/config/config.ts",
    "test": "npm test:unit && npm test:snap && npm test:e2e",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --exit-zero-on-changes",
    "docs": "typedoc",
    "version:major": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts major",
    "version:minor": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts minor",
    "version:patch": "ts-node -P tsconfig.scripts.json scripts/increase-version.ts patch",
    "update-version:major": "./scripts/update-version.sh major",
    "update-version:minor": "./scripts/update-version.sh minor",
    "update-version:patch": "./scripts/update-version.sh patch",
    "e2e:install": "playwright install --with-deps",
    "prepare": "husky"
  },
  "dependencies": {
    "@alexevs/ts-guards": "1.0.9",
    "@headlessui/react": "2.2.0",
    "@tanstack/react-router": "1.161.3",
    "immer": "10.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "recharts": "2.15.0",
    "seroval": "1.5.0",
    "zustand": "5.0.2"
  },
  "devDependencies": {
    "@alexevs/prettier-config": "1.0.0",
    "@babel/plugin-proposal-private-property-in-object": "7.21.11",
    "@chromatic-com/storybook": "3.2.7",
    "@eslint/js": "9.39.1",
    "@playwright/test": "1.57.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/blocks": "8.6.14",
    "@storybook/react": "8.6.14",
    "@storybook/react-vite": "8.6.14",
    "@storybook/test": "8.6.14",
    "@tanstack/router-devtools": "1.161.1",
    "@tanstack/router-plugin": "1.161.3",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/node": "20.19.26",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.1",
    "@typescript-eslint/eslint-plugin": "8.49.0",
    "@typescript-eslint/parser": "8.56.1",
    "@vitejs/plugin-react": "4.3.4",
    "cross-env": "10.1.0",
    "dotenv": "17.2.3",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-storybook": "0.12.0",
    "eslint-plugin-tsdoc": "0.5.0",
    "eslint-plugin-unused-imports": "4.3.0",
    "husky": "8.0.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "30.2.0",
    "jest-environment-jsdom": "30.2.0",
    "jiti": "2.6.1",
    "lint-staged": "13.3.0",
    "prettier": "3.5.3",
    "sass": "1.95.0",
    "storybook": "8.6.14",
    "storybook-addon-pseudo-states": "4.0.4",
    "ts-jest": "29.4.6",
    "ts-node": "10.9.2",
    "typedoc": "0.28.15",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "vite": "6.0.7"
  }
}
